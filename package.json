{
  "name": "daily-code",
  "version": "1.1.14",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    },
    "./node": {
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs"
    },
    "./server": {
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs"
    },
    "./browser": {
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.cjs"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.cjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.mts",
        "./dist/index.d.cts"
      ],
      "types": [
        "./dist/types.d.mts",
        "./dist/types.d.cts"
      ],
      "node": [
        "./dist/node.d.mts",
        "./dist/node.d.cts"
      ],
      "server": [
        "./dist/server.d.mts",
        "./dist/server.d.cts"
      ],
      "browser": [
        "./dist/browser.d.mts",
        "./dist/browser.d.cts"
      ],
      "react": [
        "./dist/react.d.mts",
        "./dist/react.d.cts"
      ]
    }
  },
  "scripts": {
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "tc": "tsc --noEmit --watch",
    "vite": "vite dev",
    "exports": "tsx ./write-exports.ts",
    "build": "npm run exports && tsdown --config ./tsdown.config.ts",
    "dev": "concurrently --names \"R,T\" --prefix-colors \"blue.dim,magenta.dim\" \"tsdown --config ./tsdown.config.ts --watch\" \"tsc --noEmit --watch\"",
    "start": "tsx --watch ./src/index.ts",
    "prepublishOnly": "rm -rf ./dist && npm run lint && npm run build && npm run test"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@reduxjs/toolkit": "^2.11.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-redux": "^9.2.0",
    "socket.io": "^4.8.1",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  },
  "author": "Nazmus Sayad",
  "license": "MIT",
  "homepage": "https://gh.sayad.dev/daily-code#readme",
  "repository": {
    "type": "git",
    "url": "git+https://gh.sayad.dev/daily-code.git"
  },
  "bugs": {
    "url": "https://gh.sayad.dev/daily-code/issues"
  }
}
