{
  "name": "daily-code",
  "version": "1.1.8",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepare": "husky",
    "test": "bun test",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "tc": "tsc --noEmit --watch",
    "vite": "vite dev",
    "build": "rollup -c",
    "dev": "concurrently \"rollup -c -w\" \"tsc --noEmit -w\"",
    "start": "tsx --watch ./src/index.ts",
    "prepublishOnly": "rm -rf ./dist && npm run lint && npm run build && npm run test"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@reduxjs/toolkit": "^2.9.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/bun": "^1.3.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-check-file": "^3.3.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "rollup": "^4.52.5",
    "rollup-plugin-dts": "^6.2.3",
    "socket.io": "^4.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.10"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.mjs",
      "require": "./dist/node.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.cjs"
    }
  },
  "author": "Nazmus Sayad",
  "license": "MIT",
  "homepage": "https://github.com/NazmusSayad/daily-code#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NazmusSayad/daily-code.git"
  },
  "bugs": {
    "url": "https://github.com/NazmusSayad/daily-code/issues"
  }
}
